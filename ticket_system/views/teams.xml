<odoo>
    <data>
        <record id="team_tree_view" model="ir.ui.view">
            <field name="name">Ticket System Tree View</field>
            <field name="model">ticket_system.ticket_system</field>
            <field name="arch" type="xml">
                <tree string="Teams">
                    <field name="name"/>
                    <field name="use_alias"/>
                </tree>
            </field>
        </record>
        <record id="ticket_form_view" model="ir.ui.view">
            <field name="name"></field>
        </record>
        <record id="ticket_system.team_action" model="ir.actions.act_window">
            <field name="name">Teams</field>
            <field name="model">ticket_system.ticket_system</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="help" type="html">
                <p>Create New Team</p>
            </field>
        </record>
        <menuitem name="Ticket System Teams" id="ticket_system.teams" parent="ticket_system.configration" action="ticket_system.team_action"/>
    </data>
</odoo>